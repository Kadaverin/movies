{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["react", "prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", "jsx"]
      }
    ],
    "prettier/prettier": "error",

    "import/prefer-default-export": "off",
    "max-len": ["error", 80]
  },
  "env": {
    "browser": true
  }
}
